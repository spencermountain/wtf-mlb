    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-filesize-check": "0.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-terser": "^5.0.0",
    "tap-dancer": "^0.2.0",
