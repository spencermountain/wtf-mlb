{
  "name": "wtf-plugin-mlb",
  "description": "baseball data plugin for wtf_wikipedia",
  "version": "0.0.1",
  "main": "./builds/wtf-plugin-mlb.js",
  "unpkg": "./builds/wtf-plugin-mlb.min.js",
  "module": "./builds/wtf-plugin-mlb.mjs",
  "author": "Spencer Kelly (spencermountain)",
  "homepage": "https://github.com/spencermountain/wtf_wikipedia/plugins/mlb#readme",
  "scripts": {
